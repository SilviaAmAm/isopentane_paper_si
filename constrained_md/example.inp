&GLOBAL
  RUN_TYPE MD
  PRINT_LEVEL LOW
  PROJECT_NAME example
&END GLOBAL
&MOTION
  &PRINT
    &TRAJECTORY
      &EACH
        MD 500
      &END EACH
    &END TRAJECTORY
  &END PRINT
  &MD
    ENSEMBLE NVT
    &THERMOSTAT
      TYPE CSVR
      &CSVR
      &END CSVR
    &END THERMOSTAT
    TEMPERATURE 300
    STEPS 5000
    TIMESTEP 1.0
  &END MD
  &CONSTRAINT
    &COLLECTIVE
      COLVAR 1
      TARGET [angstrom] 2.20
      INTERMOLECULAR
    &END COLLECTIVE
    &COLLECTIVE
      COLVAR 2
      TARGET [angstrom] 2.40
      INTERMOLECULAR
    &END COLLECTIVE
  &END CONSTRAINT
&END MOTION
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    LSD
    &QS
      METHOD PM6
      &SE
         ANALYTICAL_GRADIENTS T
      &END
    &END QS
    &SCF
      SCF_GUESS ATOMIC
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 20.0 20.0 20.0
    &END CELL
    &TOPOLOGY
     COORD_FILE_FORMAT XYZ
     COORD_FILE_NAME example.xyz
    &END TOPOLOGY
    &KIND C
      BASIS_SET SZV-GTH-PADE
      POTENTIAL GTH-PADE-q4
    &END KIND
    &KIND H
      BASIS_SET SZV-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND N
      BASIS_SET SZV-GTH-PADE
      POTENTIAL GTH-PADE-q5
    &END KIND
    &COLVAR
      &DISTANCE
        ATOMS 3 1
      &END DISTANCE
    &END COLVAR
    &COLVAR
      &DISTANCE
        ATOMS 3 18
      &END DISTANCE
    &END COLVAR
  &END SUBSYS
&END FORCE_EVAL
